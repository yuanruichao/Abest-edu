<div class = "modal" id = "StuInfo_modal">
	<div class = "modal-dialog">
		<div class = "modal-content">
			<form method="POST" action="/api/StuInfo">
				<input value = "{{slug}}" name = "slug" type = "hidden"></input>
				<div class = "modal-header">
					<button type = "button" class = "close" data-dismiss = "modal"><span>&times;</span><span class = "sr-only">Close</span></button>
					<h4>Student Info</h4>
				</div>
				<div class = "modal-body">
					Current Year: <input value = "{{obj.StuInfo.CurYear}}" name="CurYear" type = "year"><br><br>
					Current School: <input value = "{{obj.StuInfo.CurSchool}}" name="CurSchool" type = "text"><br><br>
					Current System: <input value = "{{obj.StuInfo.CurSystem}}" name="CurSystem" type = "text"><br><br>
					Tel: <input value = "{{obj.StuInfo.Tel}}" name="Tel" type = "tel"><br><br>
					Parent Name: <input value = "{{obj.StuInfo.ParName}}" name="ParName" type = "text"><br><br>
					Parent Tel: <input value = "{{obj.StuInfo.ParTel}}" name="ParTel" type = "tel"><br><br>
					Email: <input value = "{{obj.StuInfo.Email}}" name="Email" type = "email"><br><br>
					Student Source: <input value = "{{obj.StuInfo.StuSource}}" name="StuSource" type = "text"><br><br>
					Enter Time: <input value = "{{obj.StuInfo.EtrTime}}" name="EtrTime" type = "date"><br><br>
					Target Country: <input value = "{{obj.StuInfo.TarCountry}}" name="TarCountry" type = "text"><br><br>
					Target School Type: <input value = "{{obj.StuInfo.TarSchType}}" name="TarSchType" type = "text"><br><br>
					Target School Year: <input value = "{{obj.StuInfo.TarSchYear}}" name="TarSchYear" type = "year"><br><br>
					Target School Season: <input value = "{{obj.StuInfo.TarSchSeason}}" name="TarSchSeason" type = "text"><br><br>
				</div>
				<div class = "modal-footer">
					<button type = "submit">Create</button>	
				</div>
			</form>
		</div>
	</div>
</div>

